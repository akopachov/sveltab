{
  "name": "my-own-tab",
  "version": "1.2.2",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "engines": {
    "pnpm": "^8.0.0",
    "node": "^20.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node removeInlineScript.cjs",
    "build:web-ext:chromium": "node generateManifest.cjs chromium && web-ext build --source-dir=build --overwrite-dest --artifacts-dir=build-web-ext/chromium",
    "build:web-ext:firefox": "node generateManifest.cjs firefox && web-ext build --source-dir=build --overwrite-dest --artifacts-dir=build-web-ext/firefox",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "run:ff": "node generateManifest.cjs firefox && web-ext run --devtools --firefox=deved --source-dir=build",
    "i18n": "paraglide-js compile --project ./project.inlang --outdir ./src/i18n/generated"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@iconify/json": "^2.2.189",
    "@iconify/tailwind": "^0.1.4",
    "@iconify/utils": "^2.1.22",
    "@inlang/paraglide-js": "1.2.7",
    "@inlang/paraglide-js-adapter-vite": "^1.2.17",
    "@skeletonlabs/skeleton": "^2.9.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "browser-storage-polyfill": "^1.0.11",
    "chart.js": "^4.4.2",
    "cssnano": "^6.0.5",
    "cssnano-preset-default": "^6.0.5",
    "cssnano-preset-lite": "^3.0.2",
    "date-fns": "^3.3.1",
    "debounce": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "fast-json-patch": "^3.1.1",
    "nanoid": "^5.0.6",
    "openmeteo": "^1.1.3",
    "p-debounce": "^4.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "randomcolor": "^0.6.2",
    "svelte": "^4.2.12",
    "svelte-awesome-color-picker": "3.0.6",
    "svelte-chartjs": "^3.1.5",
    "svelte-check": "^3.6.6",
    "svelte-fast-dimension": "^1.1.0",
    "svelte-moveable": "^0.45.0",
    "svelte-use-debounce": "^0.0.2",
    "svelte-virtual-scroll-list": "^1.3.0",
    "svelte-webext-storage-adapter": "^3.0.1",
    "tailwindcss": "^3.4.1",
    "tiny-glob": "^0.2.9",
    "tslib": "^2.6.2",
    "tslog": "^4.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-run": "^0.5.1",
    "vite-plugin-tailwind-purgecss": "^0.2.0",
    "web-ext": "^7.11.0",
    "webextension-polyfill": "^0.10.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "svelte-use-debounce>svelte": "^4.0.0",
        "addons-scanner-utils>node-fetch": "^3.0.0"
      }
    },
    "overrides": {
      "tough-cookie": ">=4.1.3",
      "request": "npm:@cypress/request@^3.0.1"
    }
  },
  "type": "module"
}
